{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hannandatafactory"
		},
		"LS_ABLB_COUNTRY_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_COUNTRY'"
		},
		"LS_ABLB_FOR_DS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_FOR_DS'"
		},
		"LS_API_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_API'"
		},
		"LS_ASQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ASQL'"
		},
		"LS_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/users"
		},
		"LS_API_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hannan"
		},
		"LS_ASQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverassesment.database.windows.net"
		},
		"LS_ASQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ASQL"
		},
		"LS_ASQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hannan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hannandatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ABLB_COUNTRY_connectionString": {
						"value": "[parameters('LS_ABLB_COUNTRY_connectionString')]"
					},
					"LS_ABLB_FOR_DS_connectionString": {
						"value": "[parameters('LS_ABLB_FOR_DS_connectionString')]"
					},
					"LS_API_password": {
						"value": "[parameters('LS_API_password')]"
					},
					"LS_ASQL_password": {
						"value": "[parameters('LS_ASQL_password')]"
					},
					"LS_API_properties_typeProperties_url": {
						"value": "[parameters('LS_API_properties_typeProperties_url')]"
					},
					"LS_API_properties_typeProperties_userName": {
						"value": "[parameters('LS_API_properties_typeProperties_userName')]"
					},
					"LS_ASQL_properties_typeProperties_server": {
						"value": "[parameters('LS_ASQL_properties_typeProperties_server')]"
					},
					"LS_ASQL_properties_typeProperties_database": {
						"value": "[parameters('LS_ASQL_properties_typeProperties_database')]"
					},
					"LS_ASQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_ASQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}